<ResourceDictionary 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="ExceptionHandler.Title">Menadżer Uległ Awarii</system:String>
    <system:String x:Key="ExceptionHandler.Header">Menadżer napotkał nieoczekiwany błąd</system:String>
    <system:String x:Key="ExceptionHandler.Copy">Kopiuj Dziennik Błądów</system:String>
    <system:String x:Key="ExceptionHandler.Ignore">Zignoruj i Zamknij</system:String>
    <system:String x:Key="ExceptionHandler.Report">Zgłoś Błąd (Jedynie po Angielsku)</system:String>

